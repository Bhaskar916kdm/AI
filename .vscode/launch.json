{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "examples.extend_actions"
        },
        {
            "name": "Python: Debug extend_actions",
            "type": "module",
            "request": "launch",
            "module": "examples.extend_actions",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Debug Captcha Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "tests/test_agent_actions.py",
                "-v",
                "-k",
                "test_captcha_solver",
                "--capture=no",
                "--log-cli-level=INFO"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}